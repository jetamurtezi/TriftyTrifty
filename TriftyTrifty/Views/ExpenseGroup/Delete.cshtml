@model TriftyTrifty.DataAccess.Models.ExpenseGroup

<h2>Fshij Grup</h2>

<div class="alert alert-warning">
    A je i sigurt që dëshiron të fshish grupin "<strong>@Model.GroupName</strong>"?
</div>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Fshij</button>
    <a asp-controller="Home" asp-action="Index" class="btn btn-secondary">Anulo</a>
</form>
